@model Installation

<div class="form-group">
    <div class="col-xs-4 control-label">Order placed</div>
    <div class="col-xs-8">
        <input class="form-control date" name="OrderDate" @ViewBag.readOnly
               value="@(Model.OrderDate?.ToString("yyyy-MM-dd") ?? "")" />
    </div>
</div>

<div class="form-group">
    <div class="col-xs-4 control-label">In stock</div>
    <div class="col-xs-8">
        <input class="form-control date" name="InStockDate" @ViewBag.readOnly
               value="@(Model.InStockDate?.ToString("yyyy-MM-dd") ?? "")"/>
    </div>
</div>

<div class="form-group">
    <div class="col-xs-4 control-label">Installed date</div>
    <div class="col-xs-8">
        <input class="form-control date" name="InstalledDate" @ViewBag.readOnly
               value="@(Model.InstalledDate?.ToString("yyyy-MM-dd") ?? "")"/>
    </div>
</div>

<div class="form-group">
    <div class="col-xs-4 control-label">Archived date</div>
    <div class="col-xs-8">
        <input class="form-control date" name="InstalledDate" @ViewBag.readOnly
               value="@(Model.ArchivedDate?.ToString("yyyy-MM-dd") ?? "")" />
    </div>
</div>


<script>
        
    $(document).ready(function() {
        $(".badge[data-toggle=tooltip]").tooltip();

    });

</script>
