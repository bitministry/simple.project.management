@model Exception
@{
    if (ViewBag.ExceptionTitle == null)
    {
        ViewBag.ExceptionTitle = "Exception";
    }
}
<div class="panel panel-danger">
    <div class="panel-heading">
        <h3 class="panel-title">
            @ViewBag.ExceptionTitle
        </h3>
    </div>
    <div class="panel-body">
        @Model.Message
    </div>
</div>